{
  "name": "Better",
  "version": "1.0.0",
  "private": true,
  "description": "Build better habits",
  "main": "server.js",
  "scripts": {
    "pretest": "mocha --require setup.js --compilers js:babel-core/register test/integration/db.spec.js test/integration/server.spec.js",
    "test": "nyc mocha --require setup.js --compilers js:babel-core/register test/unit/*.spec.js && cat ./coverage/lcov.info | coveralls",
    "start": "node server/server.js",
    "local": "nodemon server/server.js"
  },
  "dependencies": {
    "bluebird": "^3.3.4",
    "body-parser": "^1.15.0",
    "cron": "^1.1.0",
    "dotenv": "^2.0.0",
    "express": "^4.13.4",
    "express-jwt": "^3.3.0",
    "jsonwebtoken": "^5.7.0",
    "moment": "^2.12.0",
    "moment-range": "^2.2.0",
    "mongoose": "^4.4.6",
    "mongoose-function": "^0.1.0",
    "morgan": "^1.7.0",
    "node-schedule": "^1.1.0",
    "react-native": "^0.21.0",
    "react-native-button": "^1.4.2",
    "react-native-lock-ios": "0.0.5",
    "react-native-progress-bar": "^0.1.2",
    "react-native-radio-buttons": "^0.9.1",
    "react-native-refreshable-listview": "^2.0.0-beta4",
    "react-native-swipeout": "^2.0.12",
    "react-native-swiper": "^1.4.3",
    "react-native-vector-icons": "^1.3.2",
    "twilio": "^2.9.0"
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "coveralls": "^2.11.12",
    "chai-as-promised": "^5.3.0",
    "enzyme": "^2.1.0",
    "fetch-mock": "^5.1.1",
    "jsdom": "^8.1.0",
    "mocha": "^2.4.5",
    "nodemon": "^1.9.1",
    "nyc": "^8.1.0",
    "q": "^1.4.1",
    "react-addons-test-utils": "^0.14.7",
    "react-dom": "^0.14.7",
    "react-native-mock": "^0.2.6",
    "request": "^2.69.0",
    "sinon": "^1.17.5",
    "supertest": "^1.2.0"
    "underscore": "^1.8.3",
    "wd": "^0.4.0"
  },
  "nyc": {
    "lines": 90,
    "statements": 90,
    "functions": 90,
    "branches": 85,
    "reporter": [
      "lcov",
      "text-summary"
    ],
    "check-coverage": true,
    "report-dir": "./coverage"
  }
}
